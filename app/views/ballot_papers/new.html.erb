<% if current_user %>
<% if @rounds %>
   <ul>
     <% @rounds.each do |round| %>
       <li><%= link_to  "#{round.cohort_name} (#{round.location}) round #{round.round_number}", new_ballot_paper_path(round_id: round.id) %></li>
     <% end %>
   </ul>
<% end %>

<% if @round %>
  <%= render partial: 'round', locals: { round: @round} %>
<% end %>
<% else %>
Please log in
<% end %>